{
  "name": "with-svelte",
  "private": true,
  "scripts": {
    "build": "dotenv -- turbo run build --ui stream",
    "dev": "dotenv -- turbo run dev",
    "preview": "dotenv -- turbo run preview",
    "lint": "turbo run lint",
    "format": "prettier --write .",
    "check-types": "turbo run check-types",
    "payload": "dotenv -- turbo payload",
    "migrate": "dotenv -- turbo migrate --filter=@repo/payload",
    "migrate:create": "dotenv -- turbo migrate:create --filter=@repo/payload",
    "migrate:status": "dotenv -- turbo migrate:status --filter=@repo/payload"
  },
  "devDependencies": {
    "prettier": "^3.5.3",
    "prettier-plugin-svelte": "^3.3.3",
    "turbo": "^2.5.0"
  },
  "packageManager": "pnpm@8.15.6",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "dotenv-cli": "^8.0.0"
  }
}
